        <div class="panel panel-default">
            <div class="panel-heading">
                Image Tables
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
   <script>
        $(document).ready(function () {
            $('#dataTables-example').DataTable({
                responsive: true,
                ajax: {
                    "url": "/api/list",
                    "dataSrc": "Images"
                },
                columns: [
                    { data: "Name" },
                    { data: "Tags[0].Value" },
                    { data: "CreationDate" },
                    { data: "ImageId" },
                    { data: "ImageLocation" },
                    { data: "ImageType" },
                    { data: "OwnerId" },
                    { data: "State" },
                    { data: "Description" },
                    { data: "VirtualizationType" }
                ]
            });
        });
    </script>
             <table class="table table-striped table-bordered table-hover compact" id="dataTables-example">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Tags</th>
                            <th>CreationDate</th>
                            <th>ImageId</th>
                            <th>ImageLocation</th>
                            <th>ImageType</th>
                            <th>OwnerId</th>
                            <th>State</th>
                            <th>Description</th>
                            <th>VirtualizationType</th>
                        </tr>
                    </thead>
                </table>
